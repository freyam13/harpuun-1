<h1>The Starters</h1>

<table>
  <tr>
    <th>Avatar</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Year</th>
    <th>Quarter</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @starters.each do |starter| %>
  <tr>
    <td><%= starter.avatar %></td>
    <td><%= starter.first_name %></td>
    <td><%= starter.last_name %></td>
    <td><%= starter.email %></td>
    <td><%= starter.year %></td>
    <td><%= starter.quarter %></td>
    <td><%= link_to 'View Profile', starter %></td>
    <td><%= link_to 'Edit', edit_starter_path(starter) %></td>
    <td><%= link_to 'Delete', starter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Starter', new_starter_path %>
